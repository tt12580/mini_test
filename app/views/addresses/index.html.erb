<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="tables">
      <%= render "table" %>
    </div>
    <%= modal title: "添加图片", size: :large do %>
      <div class="panel-body">
        <div class="picture">
          <%= render @pictures %>
        </div>
      </div>
      <div class="panel-footer", id="paginator">
        <%= paginate(@pictures, remote: true) %>
      </div>
    <% end %>
  </div>
</div>
