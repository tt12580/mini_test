<% if @address.errors.blank? %>
  var table = '#tt';
  $.get(location.href, function(html) {
    var doc = $(html).find(table);
    $(table).replaceWith(doc);
  });
  bootbox.hideAll();
<% else %>
  var html = "<%= j(render('form', remote: true)) %>";

  $('form.new_address').replaceWith(html);
<% end %>
$("#flash").html("<%= j render 'layouts/flash' %>")
